                      <xs:annotation>
                        <xs:documentation>Физическое лицо</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="Organization" type="tns:Organization">
                      <xs:annotation>
                        <xs:documentation>Организация/ИП</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:choice>
                </xs:sequence>
              </xs:choice>
            </xs:complexType>
          </xs:element>
