                        <xs:enumeration value="Представитель собственников многоквартирного дома">
                          <xs:annotation>
                            <xs:documentation>ApartmentBuildingOwnersAgent</xs:documentation>
                          </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="Единоличный собственник помещений в многоквартирном доме">
                          <xs:annotation>
