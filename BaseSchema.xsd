          <xs:restriction base="xs:string">
            <xs:enumeration value="Project">
              <xs:annotation>
                <xs:documentation>Проект</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ApprovalProcess">
              <xs:annotation>
                <xs:documentation>На утверждении</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Rejected">
              <xs:annotation>
                <xs:documentation>Отклонен</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Approved">
              <xs:annotation>
                <xs:documentation>Утвержден</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Locked">
              <xs:annotation>
